{
    "name": "apparel_store",
    "version": "1.0.0",
    "description": "eCommerce application using a MERN stack",
    "main": "backend/main.js",
    "type": "module",
    "scripts": {
        "start": "node backend/main",
        "server": "nodemon backend/main",
        "client": "npm run start --prefix ui",
        "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
        "data:seed": "node backend/data/seeder/seeder",
        "data:clear": "node backend/data/seeder/seeder --clear",
        "build": "npm install && npm install --prefix ui && npm run build --prefix ui"
    },
    "author": "Sarah Benenate",
    "license": "MIT",
    "dependencies": {
        "@paypal/paypal-server-sdk": "^0.6.1",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.7",
        "express": "^4.21.0",
        "glob": "^11.0.1",
        "jose": "^5.9.6",
        "mongodb": "^6.9.0",
        "mongoose": "^8.6.3",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "colors": "^1.4.0",
        "concurrently": "^9.0.1",
        "dotenv": "^16.4.5",
        "nodemon": "^3.1.4"
    }
}
